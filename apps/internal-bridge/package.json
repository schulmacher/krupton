{
  "name": "@krupton/internal-bridge",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev:transformer-binance-orders": "concurrently -k -n server,typecheck -c cyan,red \"tsx watch src/binanceOrdersTransformerProcess.ts --clear-screen=false\" \"tsc --noEmit --watch --preserveWatchOutput\"",
    "dev:transformer-binance-trades": "concurrently -k -n server,typecheck -c cyan,red \"tsx watch src/binanceTradesTransformerProcess.ts --clear-screen=false\" \"tsc --noEmit --watch --preserveWatchOutput\"",
    "dev:transformer-kraken-orders": "concurrently -k -n server,typecheck -c cyan,red \"tsx watch src/krakenOrdersTransformerProcess.ts --clear-screen=false\" \"tsc --noEmit --watch --preserveWatchOutput\"",
    "dev:transformer-kraken-trades": "concurrently -k -n server,typecheck -c cyan,red \"tsx watch src/krakenTradesTransformerProcess.ts --clear-screen=false\" \"tsc --noEmit --watch --preserveWatchOutput\"",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "test": "vitest",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@krupton/api-client-node": "workspace:*",
    "@krupton/api-client-ws-node": "workspace:*",
    "@krupton/config": "workspace:*",
    "@krupton/api-interface": "workspace:*",
    "@krupton/service-framework-node": "workspace:*",
    "@krupton/persistent-storage-node": "workspace:*",
    "@krupton/utils": "workspace:*",
    "@krupton/messaging-node": "workspace:*",
    "ws": "^8.18.0"
  }
}
