{
  "name": "@krupton/api-client-node",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup && tsc --emitDeclarationOnly --declarationMap --outDir dist",
    "dev": "tsup --watch --onSuccess 'tsc --emitDeclarationOnly --declaration'",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "test": "vitest",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@krupton/config": "workspace:*",
    "@krupton/service-framework-node": "workspace:*",
    "undici": "^7.16.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  }
}
